
module physics_mappings_alg_mod

    use imaginary_mod_one, only : kernel_one_type, kernel_two_type, kernel_three_type
    use imaginary_mod_two, only : kernel_four_type, &
                                  kernel_five_type, &
                                  kernel_six_type

    implicit none

    private

    public :: my_subroutine_one

contains

    subroutine my_subroutine_one(a, b)

        implicit none

        integer :: a
        integer :: b

        ! one line
        call invoke( name = "name a", kernel_one_type( a, b ), kernel_two_type( a, b ), kernel_three_type( a, b ), built_in() )
        ! no spaces
        call invoke(name="name b",kernel_one_type(a,b),kernel_two_type(a,b),kernel_three_type(a,b),built_in())

        ! arg lines
        call invoke( name = "name c", &
                     kernel_one_type( a, b ), &
                     kernel_two_type( a, b ), &
                     kernel_three_type( a, b ), &
                     built_in() )
        ! no spaces
        call invoke(name="name d",&
                    kernel_one_type(a,b),&
                    kernel_two_type(a,b),&
                    kernel_three_type(a,b),&
                    built_in())

        ! arg arg lines
        call invoke( &
                     name = "name e", &
                     kernel_one_type( a, &
                                      b ), &
                     kernel_two_type( a, &
                                      b ), &
                     kernel_three_type( a, &
                                        b ), &
                     built_in() )
        ! no spaces
        call invoke(&
                    name="name f",&
                    kernel_one_type(a,&
                                    b),&
                    kernel_two_type(a,&
                                    b),&
                    kernel_three_type(a,&
                                    b),&
                    built_in())

    end subroutine my_subroutine_one

    subroutine my_subroutine_two(a, b)

        implicit none

        integer :: a
        integer :: b

        ! like subroutine_one, but without the name keyword
        call invoke(kernel_four_type( a, b ), kernel_five_type( a, b ), kernel_six_type( a, b ), built_in() )
        call invoke(kernel_four_type(a,b),kernel_five_type(a,b),kernel_six_type(a,b),built_in())

        call invoke( kernel_four_type( a, b ), &
                     kernel_five_type( a, b ), &
                     kernel_six_type( a, b ), &
                     built_in() )
        call invoke(kernel_four_type(a,b),&
                    kernel_five_type(a,b),&
                    kernel_six_type(a,b),&
                    built_in())

        call invoke( &
                     kernel_four_type( a, &
                                      b ), &
                     kernel_five_type( a, &
                                      b ), &
                     kernel_six_type( a, &
                                        b ), &
                     built_in() )
        call invoke(&
                    kernel_four_type(a,&
                                    b),&
                    kernel_five_type(a,&
                                    b),&
                    kernel_six_type(a,&
                                    b),&
                    built_in())

    end subroutine my_subroutine_two

end module physics_mappings_alg_mod
