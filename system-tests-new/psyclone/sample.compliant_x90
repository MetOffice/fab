
module physics_mappings_alg_mod

    use imaginary_mod_one, only : kernel_one_type, kernel_two_type, kernel_three_type
    use imaginary_mod_two, only : kernel_four_type, &
                                  kernel_five_type, &
                                  kernel_six_type

    implicit none

    private

    public :: my_subroutine_one

contains

    subroutine my_subroutine_one(a, b)

        implicit none

        integer :: a
        integer :: b

        call invoke(kernel_one_type( a, b ), kernel_two_type( a, b ), kernel_three_type( a, b ), built_in() )
        call invoke(kernel_one_type(a,b),kernel_two_type(a,b),kernel_three_type(a,b),built_in())

        call invoke(kernel_one_type( a, b ), &
                     kernel_two_type( a, b ), &
                     kernel_three_type( a, b ), &
                     built_in() )
        call invoke(kernel_one_type(a,b),&
                    kernel_two_type(a,b),&
                    kernel_three_type(a,b),&
                    built_in())

        call invoke(kernel_one_type( a, &
                                      b ), &
                     kernel_two_type( a, &
                                      b ), &
                     kernel_three_type( a, &
                                        b ), &
                     built_in() )
        call invoke(kernel_one_type(a,&
                                    b),&
                    kernel_two_type(a,&
                                    b),&
                    kernel_three_type(a,&
                                    b),&
                    built_in())

    end subroutine my_subroutine_one

    subroutine my_subroutine_two(a, b)

        implicit none

        integer :: a
        integer :: b

        call invoke(kernel_four_type( a, b ), kernel_five_type( a, b ), kernel_six_type( a, b ), built_in() )
        call invoke(kernel_four_type(a,b),kernel_five_type(a,b),kernel_six_type(a,b),built_in())

        call invoke( kernel_four_type( a, b ), &
                     kernel_five_type( a, b ), &
                     kernel_six_type( a, b ), &
                     built_in() )
        call invoke(kernel_four_type(a,b),&
                    kernel_five_type(a,b),&
                    kernel_six_type(a,b),&
                    built_in())

        call invoke( &
                     kernel_four_type( a, &
                                      b ), &
                     kernel_five_type( a, &
                                      b ), &
                     kernel_six_type( a, &
                                        b ), &
                     built_in() )
        call invoke(&
                    kernel_four_type(a,&
                                    b),&
                    kernel_five_type(a,&
                                    b),&
                    kernel_six_type(a,&
                                    b),&
                    built_in())

    end subroutine my_subroutine_two

end module physics_mappings_alg_mod
